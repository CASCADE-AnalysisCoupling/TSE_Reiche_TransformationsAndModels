<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="edu.kit.kastel.sdq.coupling.evaluation.supporting.accessanalysisresultinterpreter.handlers.InterpreterHandler"
            id="edu.kit.kastel.sdq.coupling.evaluation.supporting.accessanalysisresultinterpreter.interpretercommand"
            name="AccessAnalysisResultInterpreterCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <separator
               name="edu.kit.kastel.sdq.coupling.evaluation.supporting.accessanalysisresultinterpreter.beforeseparator"
               visible="true">
         </separator>
         <menu
               id="edu.kit.kastel.sdq.coupling.evaluation.supporting.accessanalysisresultinterpreter..menu"
               label="AccessAnalysisResultInterpreter">
            <command
                  commandId="edu.kit.kastel.sdq.coupling.evaluation.supporting.accessanalysisresultinterpreter.interpretercommand"
                  label="Interpret"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeMenuSelection">
                     <iterate>
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <or>
                              <test
                                    property="org.eclipse.core.resources.name"
                                    value="*AccessAnalysisResults*">
                              </test>
                              <test
                                    property="org.eclipse.core.resources.name"
                                    value="*.repository">
                              </test>
                              <test
                                    property="org.eclipse.core.resources.name"
                                    value="*.adversary">
                              </test>
                              <test
                                    property="org.eclipse.core.resources.name"
                                    value="*.confidentiality">
                              </test>
                              <test
                                    property="org.eclipse.core.resources.name"
                                    value="*mappings.txt*">
                              </test>
                           </or>
                        </adapt>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
         </menu>
         <separator
               name="edu.kit.kastel.sdq.coupling.evaluation.supporting.accessanalysisresultinterpreter.afterseparator"
               visible="true">
         </separator>
      </menuContribution>
   </extension>
</plugin>
