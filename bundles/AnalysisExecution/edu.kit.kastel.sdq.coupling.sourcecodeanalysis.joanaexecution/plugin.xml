<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="edu.kit.kastel.sdq.coupling.sourcecodeanalysis.joanaexecution.JoanaExecutionHandler"
            id="edu.kit.kastel.sdq.coupling.sourcecodeanalysis.command.runjoanaexecution"
            name="Source Code Analysis: Execute JOANA for Access Analysis">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="edu.kit.kastel.sdq.coupling.sourcecodeanalysis.joanaexecution.JoanaExecutionHandler"
            commandId="edu.kit.kastel.sdq.coupling.sourcecodeanalysis.joanaexecution.handler.joanaexecution">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="joanaExecution"
               label="Analysis Execution"
               mnemonic="e">
            <command
                  commandId="edu.kit.kastel.sdq.coupling.sourcecodeanalysis.command.runjoanaexecution"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>

</plugin>
